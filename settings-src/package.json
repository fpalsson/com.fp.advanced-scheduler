{
  "name": "settings-src",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --mode development && cd ../ && homey app run"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "v-currency-field": "^3.1.1",
    "vue": "^2.6.12",
